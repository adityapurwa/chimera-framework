{
    "ins"     : ["a", "b"],
    "out"     : "f",
    "verbose" : true,
    "chains"  : [
        {
            "mode"  : "parallel",
            "chains" : [
                {
                    "command" : "python tests/programs/add.py",
                    "ins" : ["a", "b"],
                    "out" : "c"
                },
                {
                    "chains": [
                        {
                            "command" : "javac tests/programs/Substract.java"
                        },
                        {
                            "command" : "java -cp tests/programs/ Substract",
                            "ins" : ["a", "b"],
                            "out" : "d"
                        }
                    ]
                }
            ]
        },
        {
            "command" : "php tests/programs/multiply.php",
            "ins" : ["c", "d"],
            "out" : "e"
        },
        {
            "command" : "node tests/programs/add.js",
            "ins" : ["e", "a"],
            "out" : "f"
        }
    ]
}
